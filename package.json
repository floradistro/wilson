{
  "name": "wilson",
  "version": "1.1.0",
  "type": "module",
  "bin": {
    "wilson": "./dist/index.js"
  },
  "scripts": {
    "dev": "bun run src/index.tsx",
    "start": "bun run src/index.tsx",
    "start:enhanced": "bun run src/AppEnhanced.tsx",
    "build": "bun build src/index.tsx --outdir dist --target node --minify --external @supabase/supabase-js",
    "build:enhanced": "node scripts/build-enhanced.js",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Tests coming soon...\"",
    "lint": "echo \"Linting coming soon...\"",
    "link": "bun link",
    "clean": "rm -rf dist",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "@pppp606/ink-chart": "^0.2.4",
    "@supabase/supabase-js": "^2.39.0",
    "chalk": "^5.6.2",
    "cli-highlight": "^2.1.11",
    "ignore": "^7.0.5",
    "ink": "^6.6.0",
    "ink-text-input": "^6.0.0",
    "marked": "^17.0.1",
    "postgres": "^3.4.8",
    "react": "^19.2.3",
    "react-devtools-core": "^6.1.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "@types/react": "^19.2.8",
    "typescript": "^5.7.2"
  }
}
